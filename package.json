{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=\"0\" nuxi dev",
    "build": "nuxt build",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=\"0\" NODE_OPTIONS=--max-old-space-size=$FONTEND_NODE_OLD_HEAP node /usr/src/.output/server/index.mjs",
    "dev:clean": "nuxi cleanup .",
    "dev:prepare": "nuxi prepare .",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:write": "eslint --fix --ext .ts,.vue --ignore-path .gitignore .",
    "lint:write:diff": "eslint $(git diff --diff-filter=d --name-only HEAD | grep -E '\\.(ts|vue)$' | xargs -0 | sed 's/frontend/ ./') --fix --ignore-path .gitignore",
    "lint:write:diff:precommit": "eslint $(git diff --diff-filter=d --name-only HEAD | grep -E '\\.(ts|vue)$' | xargs -0 | sed -r 's|.*app/||') --fix --ignore-path .gitignore --config .eslintrc.precommit.js",
    "prettier": "prettier --check \"**/*.+(ts|css|vue|scss)\" --ignore-path .gitignore",
    "prettier:write": "prettier --write \"**/*.+(ts|css|vue|scss)\" --ignore-path .gitignore",
    "prettier:write:diff": "prettier --write $(git diff --diff-filter=d --name-only HEAD | grep -E '\\.(ts|css|vue|scss)$' | xargs -0 | sed 's/frontend/ ./') --ignore-path .gitignore",
    "stylelint": "stylelint --quiet '**/*{.vue,.scss}'",
    "stylelint:fix": "stylelint '**/*{.vue,.scss}' --fix --quiet",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "dev:storybook": "sb dev --no-open --quiet -p $STORYBOOK_PORT",
    "build:storybook": "sb build --output-dir ../../.storybook-output",
    "start:storybook": "npx http-server ../../.storybook-output/ -p $STORYBOOK_PORT --robots false --username $STORYBOOK_USERNAME --password $STORYBOOK_PASSWORD",
    "typecheck": "nuxi typecheck ."
  },
  "dependencies": {
    "@vueuse/core": "10.11.0",
    "nuxt": "3.12.3",
    "vue": "latest"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@storybook/addon-actions": "7.6.4",
    "@storybook/addon-essentials": "7.6.4",
    "@storybook/addon-storysource": "7.6.4",
    "@storybook/cli": "7.6.4",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/vue3": "7.6.4",
    "@storybook/vue3-vite": "7.6.4",
    "@testing-library/dom": "8.5.0",
    "@testing-library/user-event": "13.5.0",
    "@testing-library/vue": "6.6.1",
    "@types/jest": "^29.5.12",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "29.2.2",
    "eslint": "8.53.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-nuxt": "1.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-vue": "9.18.1",
    "happy-dom": "^14.12.3",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "12.5.0",
    "playwright-core": "^1.45.1",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.77.6",
    "sass-loader": "10.1.1",
    "storybook-addon-sass-postcss": "0.1.3",
    "storybook-builder-vite": "0.1.23",
    "style-loader": "2.0.0",
    "stylelint": "14.0.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-processor-html": "1.0.0",
    "stylelint-scss": "4.3.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.11.5",
    "unplugin-vue-components": "0.22.8",
    "vitest": "^1.6.0"
  }
}
